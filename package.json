{
  "name": "yaml-generator-ampersand",
  "displayName": "YAML Generator Ampersand",
  "publisher": "Ampersand",
  "description": "",
  "icon": "/assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/immdipu/yaml-generator-ampersand-extension"
  },
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "command": "yaml-generator-ampersand.openInYamlGenerator",
          "when": "editorLangId == yaml",
          "group": "navigation"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "yaml-generator-ampersand",
          "name": "YAML Generator",
          "icon": "./assets/icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "yaml-generator-ampersand.openPreview",
        "title": "Open Preview"
      },
      {
        "command": "yaml-generator-ampersand.openInYamlGenerator",
        "title": "Open in YAML Generator"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "build": "vsce package",
    "dev": "cd react && npm run dev",
    "build:react": "cd react && npm run build",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@eslint/js": "^9.15.0",
    "globals": "^15.12.0",
    "typescript-eslint": "^8.15.0"
  }
}
